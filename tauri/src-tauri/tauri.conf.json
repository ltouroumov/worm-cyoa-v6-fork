{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "worm-cyoa-v17",
  "version": "0.1.0",
  "identifier": "ch.ltouroumov.cyoa.worm17",
  "build": {
    "frontendDist": "../static",
    "devUrl": "http://localhost:8088",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "Worm CYOA V17",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
